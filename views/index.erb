<h1>My name is <%= @cat_name %> and I am <%= @cat_age %> years old</h1>
<% if !@cat_name %>
<form action = "/named_cat" method = "get">
  <input type="text" name="name">
  <input type="text" name="age">
  <input type="submit" value="Submit">
</form>
<% end %>
